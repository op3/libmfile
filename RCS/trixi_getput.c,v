#include <string.h>

#include "mfile.h"
#include "getputint.h"
#include "trixi_getput.h"

static char rcsid[] = "$Id: trixi_getput.c,v 1.1 1992/07/30 12:09:41 se Exp $";

/* $Log: trixi_getput.c,v $
 * Revision 1.1  1992/07/30  12:09:41  se
 * Initial revision
 *
 *
 */


#define fpos(s) (((level * mat->lines + line) * mat->columns + col) * (s) +512)

int trixi_get (mat, buffer, level, line, col, num)
     MFILE *mat;
     u_int level, line, col, num;
     int *buffer;
{
  return getle2 (mat->ap, buffer, fpos (2), num);
}
@


